e":"code","source":["with tf.name_scope(\"dnn\"):\n","    hidden1 = tf.layers.dense(X, n_hidden1, name=\"hidden1\",\n","                              activation=tf.nn.elu)\n","    hidden2 = tf.layers.dense(hidden1, n_hidden2, name=\"hidden2\",\n","                              activation=tf.nn.elu)\n","    hidden3 = tf.layers.dense(hidden2, n_hidden3, name=\"hidden3\",\n","                              activation=tf.nn.elu)\n","    logits = tf.layers.dense(hidden3, n_outputs, name=\"outputs\")\n","    y_proba = tf.nn.softmax(logits)"],"execution_count":0,"outputs":[]},{"metadata":{"id":"5VCkaTiP-xp5","colab_type":"code","colab":{}},"cell_type":"code","source":["with tf.name_scope(\"loss\"):\n","    xentropy = tf.nn.sparse_softmax_cross_entropy_with_logits(labels=y, logits=logits)\n","#     would cause error\n","#     xentropy = tf.nn.softmax_cross_entropy_with_logits_v2(labels=y, logits=logits)\n","    loss = tf.reduce_mean(xentropy, name=\"loss\")"],"execution_count":0,"outputs":[]},{"metadata":{"id":"qAu50bAA-xp7","colab_type":"code","colab":{}},"cell_type":"code","source":["learning_rate = 0.01\n","\n","with tf.name_scope(\"train\"):\n","    optimizer = tf.train.GradientDescentOptimizer(learning_rate)\n","    training_op = optimizer.minimize(loss)"],"execution_count":0,"outputs":[]},{"metadata":{"id":"_Yz5XixT-xp9","colab_type":"code","colab":{}},"cell_type":"code","source":["with tf.name_scope(\"eval\"):\n","    correct = tf.nn.in_top_k(logits, y, 1)\n","    accuracy = tf.reduce_mean(tf.cast(correct, tf.float32))"],"execution_count":0,"outputs":[]},{"metadata":{"id":"4-cSMwfP-xqC","colab_type":"code","colab":{}},"cell_type":"code","source":["init = tf.global_variables_initializer()\n","saver = tf.train.Saver()"],"execution_count":0,"outputs":[]},{"metadata":{"id":"0-Nq8maH-xqF","colab_type":"code","outputId":"49c455cd-7299-453c-ab66-abeb9559bcb3","executionInfo":{"status":"ok","timestamp":1556247639204,"user_tz":240,"elapsed":107646,"user":{"displayName":"Ruikang Wang","photoUrl":"https://lh6.googleusercontent.com/-sxpbEWf-1Ng/AAAAAAAAAAI/AAAAAAAAAJ4/3m9qUw3an_4/s64/photo.jpg","userId":"10914323651690992949"}},"colab":{"base_uri":"https://localhost:8080/","height":397}},"cell_type":"code","source":["%%time\n","n_epochs = 20\n","n_batches = 50\n","\n","with tf.Session() as sess:\n","    init.run()\n","    for epoch in range(n_epochs):\n","        for X_batch, y_batch in shuffle_batch(X_train, y_train, batch_size):\n","            sess.run(training_op, feed_dict={X: X_batch, y: y_batch})\n","        acc_batch = accuracy.eval(feed_dict={X: X_batch, y: y_batch})\n","        acc_valid = accuracy.eval(feed_dict={X: X_valid, y: y_valid})\n","        print(epoch, \"Batch accuracy:\", acc_batch, \"Validation accuracy:\", acc_valid)\n","\n","    save_path = saver.save(sess, \"./my_model_final.ckpt\")"],"execution_count":147,"outputs":[{"output_type":"stream","text":["0 Batch accuracy: 0.9 Validation accuracy: 0.9122\n","1 Batch accuracy: 0.92 Validation accuracy: 0.925\n","2 Batch accuracy: 0.96 Validation accuracy: 0.932\n","3 Batch accuracy: 0.9 Validation accuracy: 0.9386\n","4 Batch accuracy: 0.96 Validation accuracy: 0.9438\n","5 Batch accuracy: 0.94 Validation accuracy: 0.9496\n","6 Batch accuracy: 1.0 Validation accuracy: 0.9548\n","7 Batch accuracy: 0.94 Validation accuracy: 0.9574\n","8 Batch accuracy: 0.96 Validation accuracy: 0.9606\n","9 Batch accuracy: 0.92 Validation accuracy: 0.9616\n","10 Batch accuracy: 0.92 Validation accuracy: 0.9634\n","11 Batch accuracy: 1.0 Validation accuracy: 0.9662\n","12 Batch accuracy: 0.98 Validation accuracy: 0.9658\n","13 Batch accuracy: 0.96 Validation accuracy: 0.9684\n","14 Batch accuracy: 1.0 Validation accuracy: 0.97\n","15 Batch accuracy: 0.94 Validation accuracy: 0.971\n","16 Batch accuracy: 0.98 Validation accuracy: 0.9706\n","17 Batch accuracy: 1.0 Validation accuracy: 0.9702\n","18 Batch accuracy: 0.94 Validation accuracy: 0.974\n","19 Batch accuracy: 0.98 Validation accuracy: